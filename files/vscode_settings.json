{
  "window.newWindowDimensions": "inherit",
  "window.closeWhenEmpty": true,
  "window.title": "${activeEditorShort}${separator}${rootPath}",
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 120,
  "editor.minimap.maxColumn": 180,
  "editor.multiCursorModifier": "alt",
  "editor.tabCompletion": "on",
  "editor.formatOnType": true,
  "editor.lineNumbers": "on",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "",
        "settings": {
          "foreground": "#ffffff",
          "fontStyle": "italic bold underline"
        }
      }
    ]
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "300",
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.cursorBlinking": true,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "sync.gist": "c13b96c1db821afa17aa018af227ad68",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.associations": {
    "*.js": "javascriptreact",
    "*.html.twig": "twig",
    "*.svelte": "svelte",
    "*.svx": "markdown",
    ".editorconfig": "editorconfig",
    "*.svtext": "svelte",
    "*.sass": "sass"
  },
  "emmet.includeLanguages": {
    "twig": "html"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.validate": [
    "javascript"
  ],
  "markdown.styles": [
    "markdown.css"
  ],
  "[markdown]": {
    "editor.quickSuggestions": true,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "vetur.format.defaultFormatter.less": "none",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Darcula Theme from IntelliJ",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.fontAliasing": "antialiased",
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#f8a62b",
    "editor.background": "#302f2f",
    "statusBar.background": "#424242"
  },
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "intelephense.format.enable": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "sass.format.setPropertySpace": false,
  "scss.validate": false,
  "sass.andStared": [
    "active",
    "focus",
    "hover",
    "nth-child"
  ],
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "svelte.plugin.svelte.compilerWarnings": {
    "unused-export-let": "error"
  },
  "extensions.ignoreRecommendations": false,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.run": "onSave",
  "eslint.format.enable": true,
  "security.workspace.trust.untrustedFiles": "open",
  "prettier.useTabs": true,
  "prettier.enable": true,
  "prettier.jsxSingleQuote": true,
  "prettier.jsxBracketSameLine": true
}